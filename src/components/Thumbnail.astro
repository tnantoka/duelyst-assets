---
import { units } from '../scripts/resources';
import Frame from './Frame.astro';

export interface Props {
  unitName: string;
}

const { unitName } = Astro.props;

const unit = units[unitName];
---

{
  unit.gif !== undefined ? (
    <img src={unit.gif} />
  ) : (
    <Frame unitName={unitName} />
  )
}
